{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Error",
      "Host.Results": "Information",
      "Function": "Information",
      "SqlCollaborative.Azure.DataPipelineTools": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception",
        "enableLiveMetrics": true
      }
    }
  }
}